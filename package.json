{
    "name": "l2h-live",
    "displayName": "l2h-live",
    "description": "",
    "version": "0.0.1",
    "publisher": "project-pp",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:l2h-live.openPreview"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "l2h-live.openPreview",
                "title": "Open LaTeX live preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "latex.js": "^0.9.2"
    }
}
